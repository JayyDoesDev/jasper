{
    "name": "jasper",
    "version": "2.5.2",
    "scripts": {
        "build": "gulp js && tsc",
        "start": "node dist/Source/index.js",
        "dev": "yarn build && yarn eslint && node dist/Source/index.js",
        "eslint": "eslint \"**/*.{ts,mjs,cjs}\"",
        "builddocker": "docker build -t jasper .",
